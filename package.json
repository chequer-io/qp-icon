{
  "name": "querypie-icon",
  "version": "0.0.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepare-generate": "ts-node -r tsconfig-paths/register -T scripts/prepareGenerate.ts",
    "generate": "npm run prepare-generate && ts-node -r tsconfig-paths/register -T scripts/generate.ts",
    "storybook": "npm run generate && start-storybook -p 6006",
    "build": "rm -rf dist && npm run generate && ttsc -p tsconfig.build.json && babel dist -d dist",
    "clean": "rm -rf node_modules/ package-lock.json && npm cache clean -f && npm i",
    "prepublishOnly": "npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "directory-tree": "^3.2.0",
    "fuse.js": "^6.5.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toastify": "^8.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-controls": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@types/node": "17.0.15",
    "@types/prettier": "^2.4.4",
    "@types/react": "17.0.39",
    "@types/rimraf": "^3.0.2",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.5.6",
    "husky": "^7.0.4",
    "lint-staged": "^10.2.13",
    "prettier": "^2.5.1",
    "tsconfig-paths": "^3.12.0",
    "ttypescript": "^1.5.13",
    "typescript": "4.5.5",
    "typescript-transform-paths": "^3.3.1"
  }
}
